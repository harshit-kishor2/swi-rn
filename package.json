    "tcp": "adb reverse tcp:8081 tcp:8081",
    "android:tcp": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "apk:debug": "cd android && ./gradlew assembleDebug && cd ..",
    "apk:release": "cd android && ./gradlew assembleRelease && cd ..",
